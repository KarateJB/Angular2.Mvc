<!DOCTYPE html>
<html>
<head>
    <base href="/">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>


    <script src="~/lib-npm/reflect-metadata/Reflect.js"></script>
    <script src="~/lib-npm/zone.js/zone.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib-npm/es6-shim/es6-shim.js"></script>
    @*<script src="~/lib-npm/traceur/traceur.js"></script>*@
    <script src="~/lib-npm/systemjs/system.src.js"></script>
    <script src="~/app/systemjs.config.js"></script>

    
    @*<jl-javascript-logger-definitions />*@
    <script src="~/lib-npm/jsnlog/jsnlog.min.js"></script>
    <script src="~/lib-npm/sweetalert2/sweetalert2.min.js"></script>
    <link href="~/lib-npm/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib-npm/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/app/component/blockUI/blockUI.comp.css" rel="stylesheet" /><!--Must override bootstrap-->

</head>

<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">Demo</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="About">About</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Contact">Contact</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        <br /><br /><br />
        @RenderBody()
        @RenderSection("scripts", required: false)
        <hr />
        <footer>
            <p>&copy; 2016 - Angular2.Mvc</p>
        </footer>
    </div>


    
</body>

</html>